#CSE-Assignment 01

#include<stdio.h>
main(){
    
    int row,col,count=1;
    for(row=1;row<=5;row++){
        
        for(col=1;col<=row;col++)
        printf("%d ", count++);
        printf("\n");
    } 
}

#CSE-Assignment 02

#include<stdio.h>

main(){

    int i,j;

//Patterns
    for(j=5; j>=1; j--){

        for(i=5; i>=j; i--)
            printf("*");

        printf("\n");
    }
}
